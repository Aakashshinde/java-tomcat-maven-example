node{
    stage('SCM'){
        git 'https://github.com/Aakashshinde/java-tomcat-maven-example.git'
    }
    stage('Maven'){
        sh 'mvn package'
    }
    stage('Archive'){
        archiveArtifacts artifacts: 'target/java-tomcat-maven-example.war', excludes: 'target', followSymlinks: false
    }
}
